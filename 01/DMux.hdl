/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
      Not(in=in, out=notin);
      Or(a=notin, b=sel, out=w1);
      Not(in=w1, out=a);

      Nand(a=in, b=sel, out=w2);
      Not(in=w2, out=b);
}
